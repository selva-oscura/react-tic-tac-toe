{
  "name": "Tic-Tac-Toe",
  "version": "0.2.1",
  "description": "Tic-Tac-Toe -- Progressive Web App, made with node, React, service workers, and storage of state & app shell in the browser",
  "homepage": "https://selva-oscura.github.io/react-tic-tac-toe",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selva-oscura/react-tic-tac-toe.git"
  },
  "keywords": [
    "JS",
    "React",
    "create-react-app",
    "PWA",
    "sw-precache"
  ],
  "author": "Carol  St. Louis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/selva-oscura/react-tic-tac-toe/issues"
  },
  "private": true,
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "react-scripts": "0.7.0",
    "sw-precache": "^4.2.2"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp manifest.json build/ && sw-precache --root='build/' --static-file-globs='build/**/!(*map*)'",
    "deploy" : "npm run build&&gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
